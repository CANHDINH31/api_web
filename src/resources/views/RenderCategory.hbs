<div class="wrapper">
  <div class="container">
    <button type="button" class="mt-5 btn btn-success">
      {{images.0.name}} <span class="badge badge-light">Số lượng : {{count}}</span>
    </button>
    <div class="row">
      {{#each images}}
      <div class="col-lg-3 col-6">
        <img class="library_link mt-3 mb-3" src="{{this.image}}" alt="">
      </div>
      {{/each}}
    </div>
  </div>
</div>